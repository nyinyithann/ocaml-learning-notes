   "source": [
    "#require \"Base\" ;;\n",
    "open Base;;\n",
    "\n",
    "let (let*) o f = Option.bind o ~f;;\n",
    "let return = Option.return;;\n",
    "let sum_hds () =\n",
    "    let* x = List.hd [] in\n",
    "    let* y = List.hd [4;5;6] in\n",
    "    return (x + y);;\n",
    "sum_hds()"
